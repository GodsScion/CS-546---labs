<article class="new-post">
    
    {{#if data.name}}
        <h1>{{data.name}}</h1>
    {{else}}
        <h1>N/A</h1>
    {{/if}}


    {{#if data.image.medium}}
        <img src="{{data.image.medium}}" alt="Movie Picture">
    {{else}}
        <img src="data:image/png;base64," alt="N/A">
    {{/if}}



    <dl>
        <dt>Language</dt>
        {{#if data.language}}
            <dd>{{data.language}}</dd>
        {{else}}
            <dd>N/A</dd>
        {{/if}}

        

        <dt>Genres</dt>
        {{#if data.genres}}
            <dd>
                <ul>
                    {{#each data.genres}}
                    <li>
                        {{{this}}}
                    </li>
                    {{/each}}
                </ul>
            </dd>        
        {{else}}
            <dd>N/A</dd>
        {{/if}}


        <dt>Average Rating</dt>
        {{#if data.rating.average}}
            <dd>{{data.rating.average}}</dd>
        {{else}}
            <dd>N/A</dd>
        {{/if}}
        
        
        
        <dt>Network</dt>
        {{#if data.network.name}}
            <dd>{{data.network.name}}</dd>
        {{else}}
            <dd>N/A</dd>
        {{/if}}
        
        
        
        <dt>Summary</dt>
        {{#if data.summary}}
            <dd>{{{data.summary}}}</dd>
        {{else}}
            <dd>N/A</dd>
        {{/if}}
    </dl>

</article>